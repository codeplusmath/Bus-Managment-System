#:import Snackbar kivymd.uix.snackbar.Snackbar

<Separator@Widget>:
    canvas:
        Color:
            rgba: 0, 0, 0, .4
        Rectangle:
            pos: self.pos
            size: self.size

<HSeparator@Separator>:
    size_hint_y: None
    height: dp(1)

<MySeparator@Widget>:
    canvas:
        Color:
            rgba: 0, 0, 0, 0
        Rectangle:
            pos: self.pos
            size: self.size

<HoSeparator@MySeparator>:
    size_hint_y: None
    height: dp(1)


<Home>:
    BoxLayout:
        id: Home
        orientation: 'vertical'
        spacing: 20

        MDToolbar:
			title: 'Bus Managment System'
            right_action_items: [["logout",lambda x: root.disconnect()]]
            elevation: 8
            font_size: 16
        Widget:

	ScrollView:
        do_scroll_x: False
        do_scroll_y: True
        type:['content']
        pos_hint: {"center_y": .394,"center_x": .52}
			
		BoxLayout:
			orientation: 'vertical'
			pos_hints: (None, None)
			size_hint_y: None
			spacing:20
			padding: [20,30,25,150]
            height: self.minimum_height
			
			MDLabel:
                text: 'Home'
                font_name: 'VarelaRound'
                font_size: '20sp'
                color: rgba("#0e1574ff")
						
			BoxLayout:
                orientation: 'horizontal'
                size_hint_y:None
                height: self.minimum_height
                pos_hint: {"center_x": .55,"center_y": .5}
                spacing:10
				MDRaisedButton:
					valign:'center'
					text: 'Bus Details'
					font_size: '20sp'
					size_hint_y: None
					font_name: 'VarelaRound'
					on_release: root.gobusdetails()
				
				MDRaisedButton:
					valign:'center'
					text: 'Station'
					font_size: '20sp'
					size_hint_y: None
					font_name: 'VarelaRound'
					on_release: root.gostation()
				
				MDRaisedButton:
					valign:'center'
					text: 'Employee'
					font_size: '20sp'
					size_hint_y: None
					font_name: 'VarelaRound'
					on_release: root.goemployee()
				
				MDRaisedButton:
					valign:'center'
					text: 'Ticket Counter'
					font_size: '20sp'
					size_hint_y: None
					size_hint_y: None
					font_name: 'VarelaRound'
					on_release: root.goticketcounter()
			
				MDRaisedButton:
					valign:'center'
					text: 'Bus Haults'
					font_size: '20sp'
					size_hint_y: None
					size_hint_y: None
					font_name: 'VarelaRound'
					on_release: root.gobushaults()
			

<Station>:
    BoxLayout:
        id: Station
        orientation: 'vertical'
        spacing: 20

        MDToolbar:
			title: 'Bus Managment System'
			left_action_items: [["arrow-left",lambda x: root.back()]]
            right_action_items: [["logout",lambda x: root.disconnect()]]
            elevation: 8
            font_size: 16
        Widget:

    ScrollView:
        do_scroll_x: False
        do_scroll_y: True
        type:['content']
        pos_hint: {"center_y": .394 ,"center_x": .52}
        BoxLayout:
            orientation: 'vertical'
            padding: [20,30,25,150]
            size_hint_y:None
            height: self.minimum_height
            spacing: 20
			
            MDLabel:
                text: 'Station'
                font_name: 'VarelaRound'
                font_size: '20sp'
                text_size: root.width-14, 14
                color: rgba("#0e1574ff")

            BoxLayout:
                orientation: 'vertical'
                size_hint_y:None
                height: self.minimum_height
				pos_hint: {"center_y": .5 ,"center_x": .52}
                spacing:10
				padding: [0,0,50,0]
                
				MDTextField:
					id: stationcode
					multiline:False
					required: True
					hint_text: 'Station Code'
					mode: 'fill'
					font_size: '16sp'
					font_name: 'Mukta-SemiBold'
					background_color: 1,1,1,0.5
					size_hint_y:None
					size:
						self.texture_size
					size: dp(200),dp(40)
				
				MDTextField:
					id: stationname
					multiline:False
					required: True
					hint_text: 'Station Name'
					mode: 'fill'
					font_size: '16sp'
					font_name: 'Mukta-SemiBold'
					background_color: 1,1,1,0.5
					size_hint_y:None
					size:
						self.texture_size
					size: dp(200),dp(40)

				MDTextField:
					id: location
					multiline:False
					required: True
					hint_text: 'Station Location'
					mode: 'fill'
					font_size: '16sp'
					font_name: 'Mukta-SemiBold'
					background_color: 1,1,1,0.5
					size_hint_y:None
					size:
						self.texture_size
					size: dp(200),dp(40)
				
				BoxLayout:
					orientation: 'horizontal'
					size_hint_y:None
					height: self.minimum_height
					spacing:10
					
					MDRaisedButton:
						valign:'center'
						text: 'Add Station'
						font_size: '20sp'
						font_name: 'VarelaRound'
						on_release: root.addstation(stationcode.text, stationname.text, location.text)
						
						
					MDRaisedButton:
						valign:'center'
						text: 'Delete Station'
						font_size: '20sp'
						font_name: 'VarelaRound'
						on_release: print('function need to implement ')
					
					MDRaisedButton:
						valign:'center'
						text: 'Update Station'
						font_size: '20sp'
						font_name: 'VarelaRound'
						on_release: print('function need to implement ')
						
					MDRaisedButton:
						valign:'center'
						text: 'View Stations'
						font_size: '20sp'
						font_name: 'VarelaRound'
						on_release: root.viewstation()
					
